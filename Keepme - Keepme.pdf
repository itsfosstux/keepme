/* NEW STUFF FOR WATERMARK */
#watermark-container {
  position: absolute;
  width: 100%;
  top: 30%;
  z-index: 99;
  opacity: 0.4;
}

#watermark {
  /*position: absolute;
  top: 20%;
  left: 5%;
  right: 5%;*/
  position: relative;
  margin: auto;
  width: 60%;
  color: #e1e1e1;
}

#watermark h1 {
  text-align: center;
  font-size: 50px;
  line-height: 60px;
  -webkit-transform: rotate(-30deg);
  -webkit-backface-visibility: hidden;
}

/* Invoices */

.invoice-pdf {
  padding: 90px 30px;
  font-family: &#39;Helvetica Neue&#39;, Helvetica, Arial, sans-serif;
}
.invoice-pdf table {
  width: 100%;
  margin-top: 50px;
  margin-bottom: 70px;
}
.invoice-pdf table th,
.invoice-pdf table td {
  border: none;
  padding: 3px 10px;
  text-align: right;
}
.invoice-pdf table th:first-child,
.invoice-pdf table td:first-child {
  text-align: left;
}

